{
  "expo": {
    "name": "maritaca",
    "slug": "maritaca",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#C742A1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.pinapps.maritaca",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#C742A1"
      },
      "permissions": [
        "READ_PHONE_STATE"
      ]
    },
    "platforms": [
      "android"
    ]
  }
}
